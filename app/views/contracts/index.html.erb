<p id="notice"><%= notice %></p>

<h1>Listing Contracts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Ref</th>
      <th>Region</th>
      <th>Uploadedby</th>
      <th>Savings</th>
      <th>Dept</th>
      <th>Supplier</th>
      <th>Validfrom</th>
      <th>Validto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contracts.each do |contract| %>
      <tr>
        <td><%= contract.title %></td>
        <td><%= contract.ref %></td>
        <td><%= contract.region %></td>
        <td><%= contract.uploadedBy %></td>
        <td><%= contract.savings %></td>
        <td><%= contract.dept %></td>
        <td><%= contract.supplier %></td>
        <td><%= contract.validFrom %></td>
        <td><%= contract.validTo %></td>
        <td><%= link_to 'Show', contract %></td>
        <td><%= link_to 'Edit', edit_contract_path(contract) %></td>
        <td><%= link_to 'Destroy', contract, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contract', new_contract_path %>
